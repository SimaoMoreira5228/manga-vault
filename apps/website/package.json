{
	"name": "website",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "bun run codegen && vite dev",
		"build": "bun run codegen && vite build",
		"lint": "bun run codegen && eslint . && bun run check && prettier --check \"**/*\" -u",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && bun run codegen",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"codegen": "graphql-codegen"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.33.0",
		"@graphql-codegen/cli": "^5.0.7",
		"@graphql-codegen/client-preset": "^4.8.3",
		"@graphql-codegen/typescript": "^4.1.6",
		"@graphql-codegen/typescript-operations": "^4.6.1",
		"@graphql-codegen/typescript-urql": "^4.0.1",
		"@lucide/svelte": "^0.539.0",
		"@skeletonlabs/skeleton": "^3.1.8",
		"@skeletonlabs/skeleton-svelte": "^1.4.0",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.29.1",
		"@sveltejs/vite-plugin-svelte": "^6.1.2",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.11",
		"@urql/svelte": "^4.2.3",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"graphql": "^16.11.0",
		"graphql-code-generator": "^0.18.2",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.38.1",
		"svelte-check": "^4.3.1",
		"svelte2tsx": "^0.7.42",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.39.1",
		"vite": "^7.1.2",
		"zod": "^4.0.17"
	}
}
